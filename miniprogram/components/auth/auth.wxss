@import "/style/weui.wxss";
.weui-auth-dialog{
  visibility:hidden;
  opacity:0;
  transition:all .3s;
}
.weui-half-screen-dialog{
  transition:all .3s;
  transform:translateY(100%);
}
.weui-auth-dialog_show{
  visibility:visible;
  opacity:1;
}
.weui-auth-dialog_show .weui-half-screen-dialog{
    transform:translateY(0);
}
